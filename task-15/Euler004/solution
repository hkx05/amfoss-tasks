def is_palindrome(number):
    return str(number) == str(number)[::-1]

palindrome_products = [] #list for palindrome products
#palindrome product of three digit nos.
for i in range(100, 1000):
    for j in range(100, i + 1):
        product = i * j
        if is_palindrome(product):
            palindrome_products.append(product)

# Sort palindrome products in descending order
palindrome_products.sort(reverse=True)

n = int(input())

# Find and print the largest palindrome products for each test case
for k in range(n):
    limit = int(input())
    for palindrome in palindrome_products:
        if palindrome < limit:
            print(palindrome)
            break
